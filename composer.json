{
    "name": "torinaki/amp-bundle",
    "description": "Amp support for Symfony",
    "type": "symfony-bundle",
    "require": {
        "symfony/http-kernel": "^3.4",
        "symfony/dependency-injection": "^3.4",
        "symfony/config": "^3.4",
        "amphp/aerys": "^0.7"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Dmitry Balabka",
            "email": "dmitry.balabka@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": { "Amp\\AmpBundle\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5"
    }
}
